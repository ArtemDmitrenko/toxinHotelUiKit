        +card-login(cardLogin)
        +card-room-detail(roomDetail)
    .cards__flex-column-wrapper
        .cards__room-slider-margin
            +slider-room(sliderRoom888)
        .cards__room-slider-margin
            +slider-room(sliderRoom840)

