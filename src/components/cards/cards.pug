mixin cards({searchRoom={}, cardRegister={},cardLogin={}, roomDetail={}, sliderRoom840={}, sliderRoom888={}}={})
    .cards__flex-column-wrapper
        +card-search-room(searchRoom)
        +card-register(cardRegister)
